Extension { #name : #BlElementEffect }

{ #category : #'*Bloc-Alexandrie' }
BlElementEffect >> aeDrawAfter: aBlElement on: anAeCanvas [ 
	"Draw the effect on top of the element"
]

{ #category : #'*Bloc-Alexandrie' }
BlElementEffect >> aeDrawBefore: aBlElement on: anAeCanvas [ 
	"Draw on the canvas the effect behind the element"
]
